.container_main {position: relative; width: 100%; z-index: 1;}
.content {min-height: 800px; padding-bottom: 90px;}

.breadcrumb {position: relative; background-color: #fff; border-top: 1px solid #d7d7d7; border-bottom: 1px solid #d7d7d7;}
.breadcrumb > .inner {max-width: 1200px; width: 100%; margin: 0 auto;}
.breadcrumb .list_bread:after {content: ""; display: table; clear: both;}
.breadcrumb .list_bread > li {float: left; position: relative; border-right: 1px solid #d7d7d7;}
.breadcrumb .d_home {width: 50px; height: 50px; line-height: 50px; text-align: center; border-left: 1px solid #d7d7d7; border-right: 1px solid #d7d7d7;}
.breadcrumb .d_sub a {display: block; width: 168px; height: 50px; padding-left: 18px; font-size: 15px; line-height: 50px; color: #777; /*text-shadow: 1px 1px 1px #c35500;*/}
.breadcrumb .d_sub > a {background: url(../img/common/bg_select_2.png) no-repeat right top; -webkit-background-size: auto 100%; background-size: auto 100%;}
.breadcrumb .d_cur > a{font-weight: bold; color: #555;}
.breadcrumb .wrap_sub {position: absolute; top: 50px; display: none; margin-left: -1px; padding-top: 10px; padding-bottom: 5px; background-color: #fff; border: 1px solid #d7d7d7; border-top: none;}
.breadcrumb .wrap_sub:before {content:""; position: absolute; top: 0; left: 50%; display: block; width: 150px; height: 1px; margin-left: -75px; background-color: #dadee2;}
.breadcrumb .wrap_sub a:hover {color: #fff; background-color: #c60824;}
.breadcrumb .m_cur {display: none; height: 1px; overflow: hidden;}
.breadcrumb .d_sub.open .wrap_sub {display: block;}
.breadcrumb .d_sub.open > a {background-image: url(../img/common/bg_select_2_open.png);}

.breadcrumb.on {position: fixed; top: 0; left: 0; width: 100%; z-index: 2;}

.spot {display: table; width: 100%; height: 240px; margin-bottom: 65px; background-repeat: no-repeat; background-position: center center; -webkit-background-size: cover; background-size: cover;}
.spot .inner {display: table-cell; text-align: center; vertical-align: middle;}
.spot .tit {display: inline-block; min-width: 100px; padding: 3px 2px 1px; font-size: 18px; color: #fff; border: 3px solid #c60824; background: #c60824;}
.spot .txt {margin-top: 26px; padding: 0 20px; font-size: 35px; line-height: 43px; color: #222;}
.spot .txt strong {font-family: 'nanum-square-b';}
.spot.m_1 {background-image: url(../img/common/img_spot_1.jpg);}

/* footer */
.footer {position: relative; background: #ffffff; border-top: 1px solid #e5e5e5; z-index: 9;}
.footer .inner {position: relative; max-width: 1200px; width: 100%; margin: 0 auto;}
.footer .inner:after {clear: both; display: block; content:""; height: 0; visibility:hidden;}
.footer .logo {position: absolute; top: 42px; left: 20px;}
.footer .wrap_info {float: left; padding: 30px 0 30px 185px;}
.footer .term:after {content:""; clear: both; display: block; height: 0; visibility:hidden;}
.footer .term a {font-size: 13px; color: #575d67;}
.footer .term > li {float: left; padding: 0 10px; background: url(../img/common/line_v.png) 100% 50% no-repeat;}
.footer .term > li.btn_pop {background: none;}
.footer .term .link_1 {font-weight: bold; color: #df2028;}
.footer .address {padding-left: 10px; padding-right: 310px; padding-top: 5px; color: #898d92; font-size: 13px;}
.footer .mark {position: absolute; top: 66px; right: 15px;}
.footer .mark .mk_item {margin-left: 10px;}

