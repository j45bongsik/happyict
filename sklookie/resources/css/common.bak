/* control */
.control-btns {margin-top: 130px; text-align: center;}
.control-btns .page {display: inline-block; width: 10px; height: 10px; margin-right: 7px; vertical-align: middle; border-radius: 25px; background: #ffffff; -webkit-transition: width 0.3s ease-out; transition: width 0.3s ease-out;}
.control-btns .page.on {width: 22px; background: #c60824;}
.control-btns .btn-play, .control-btns .btn-stop {display: none;}
.control-btns .btn-play.show, .control-btns .btn-stop.show {display: inline-block;}

.lay-fix {overflow: hidden}

/* section */
.sect-t1 {position: relative; width: 100%; max-width: 1000px; margin: 0 auto;}
.sect-t1:after {content: ""; display: table; clear: both;}
.sect-ln1 {margin-bottom: 70px; padding-bottom: 70px; border-bottom: 1px solid #e2e2e2;}
.sect-ln1:after {content: ""; display: table; clear: both;}
.inner-cont {position: relative; max-width: 1000px; width: 100%; margin: 0 auto;}
.inner-cont:after {content: ""; display: table; clear: both;}

/* popup */
.pop-familysite {position: absolute; left: 0; bottom: 153px; display: none; width: 100%; text-align: left; background: #ffffff; border-left: 1px solid #e6e6e6; border-right: 1px solid #e6e6e6; border-bottom: 1px solid #e6e6e6; z-index: 101;}
.pop-familysite .btn-close {position: absolute; top: 20px; right: 20px;}
.pop-familysite .tit_family {position: relative; padding: 15px 25px; color: #ffffff; background: #4f5154;}
.pop-familysite .list_family::-webkit-scrollbar {display: none;}
.pop-familysite .list_family .tit_family_2 {position: relative; display: block; padding: 15px 25px; font-family: 'nanum-square-b'; font-size: 18px; color: #d12128; border-bottom: 1px solid #dcdddf;}
.pop-familysite .list_family .d-1 {float: left;}
.pop-familysite .list_family .d-2 {margin-top: 0; padding-top: 20px;}
.pop-familysite .list_family .d-2 a {display: block; padding: 5px 25px; font-size: 14px; font-weight: normal; line-height: 23px;}
.pop-familysite .list_family .d-2 a:hover, 
.pop-familysite .list_family .d-2 a:focus {color: #d12128; background: url(../img/common/ic_focus.png) 91% 50% no-repeat;}
.pop-familysite .list_family .d-2 dt {min-height: 50px; padding: 5px 25px; font-size: 16px; line-height: 23px; color: #000;}
.pop-familysite .list_family .d-2 dd {margin-left: 0;}
.pop-familysite .area_aff {width: 75%; border-right: 1px solid #dcdddf; -webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box;}
.pop-familysite .area_aff .d-2 {float: left; width: 33.3%;}
.pop-familysite .area_subs {width: 25%;}

.pop-global {position: fixed; top: 0; left: 0; display: none; width: 100%; z-index: 100;}
.pop-global .inner {position: relative; max-width: 1200px; width: 100%; margin: 0 auto;}
.pop-global .inner .pos {position: absolute; top: 68px; right: 25px; width: 110px;}
.pop-global .btn-close {position: absolute; top: 27px; right: -30px;}
.pop-global .ly-inner {padding: 10px 0; background: #ffffff;}
.pop-global .list_global a {display: block; margin: 0 20px; padding: 4px 0; font-weight: bold; color: #51555d; background: none;}
.pop-global .list_global .on a {color: #df242b; background: url(../img/common/line_global.png) 100% center no-repeat;}
.pop-global .ico_arrow {display: block; width: 100%; height: 26px; background: url(../img/common/uti_ic_arrow_1.png) center center no-repeat;}

.pop-sitemap {position: fixed; left: 0; top: 0; display: none; width: 100%; z-index: 100;}
.pop-sitemap .inner {position: relative; max-width: 1200px; width: 100%; margin: 0 auto;}
.pop-sitemap .pos {position: absolute; top: 68px; right: 25px; width: 100%; background: #ffffff;}
.pop-sitemap .btn-close {position: absolute; top: 27px; right: -30px;}
.pop-sitemap .btn-sitemap {position: absolute; right: 30px; bottom: 30px; margin: 0; padding: 5px 10px 5px 30px; color: #51555d; border: 1px solid #a8aaae; background: url(../img/common/ic_menu.png) 10px 50% no-repeat; }
.pop-sitemap .list_sitemap {width: 100%;}
.pop-sitemap .list_sitemap:after {clear: both; display: block; content:""; height: 0; visibility:hidden;}
.pop-sitemap .list_sitemap .d-1 {float: left; width: 20%; min-height: 367px; padding: 50px 0; background: url(../img/common/gra_line.png) 0% 0 repeat-y;}
.pop-sitemap .list_sitemap .d-1 div {float: none; padding-left: 53px;}
.pop-sitemap .list_sitemap .d-1 .tit {display: block; margin-bottom: 20px; font-family: 'nanum-square-b'; font-size: 24px; line-height: 38px; color: #51555d;}
.pop-sitemap .list_sitemap .d-2 {padding-top: 20px; background: url(../img/common/sitemap_line.png) 0 0 no-repeat;}
.pop-sitemap .list_sitemap .d-2 a {float: none; margin: 0; font-size: 18px; color: #51555d; background: none;}
.pop-sitemap .list_sitemap .d-2 li {float: none; padding: 6px 0;}

/* dim */
.dim-pop, .dim-menu-m {position: fixed; background: #000; opacity: 0; z-index: 9; -webkit-transition-duration: 0.3s; transition-duration: 0.3s; -webkit-transition-timing-function: ease-out; transition-timing-function: ease-out;}
.dim-pop.show {top: 0; bottom: 0; left: 0; right: 0; opacity: 0.7;}

/*---------- layout ----------*/
#wrap {position: relative; overflow: hidden;}

/* header */
.header {width: 100%; height: 114px; background: #fff /*url(../img/main/grid_line.png) 0 100% repeat-x*/; z-index: 9;}
.header .inner {max-width: 1200px; width: 100%; margin: 0 auto; padding-top: 25px;}
.header .logo {float: left; position: relative; z-index: 2;}
.header .util-menu {float: right; position: relative; padding-top: 34px; color: #c60824; z-index: 2;}
.header .util-menu a {margin-left: 30px;}
.header .util-menu li {float: left; position: relative;}
.header .util-menu .util-1 {height: 22px; padding-top: 2px; font-size: 14px;}
.header .area_global_m {display: none;}
.header .footer-m {display: none;}

.lnb {position: absolute; top: 42px; left: 0; width: 100%; padding-bottom: 11px; text-align: center; background-color: #fff;}
.lnb .innerLnb {position: relative; overflow: hidden;}
.lnb .wrapSub {position: absolute; top: 50px; left: 0; width: 100%; text-align: left; background-color: #fff; border-bottom: 1px solid #c60824; z-index: 2;}
.lnb .wrapSub .inner {width: 1200px; margin: 0 auto; padding: 50px 0;}
.lnb .wrapSub .inner:after {content:""; clear: both; display: table;}
.lnb .mnBg {position: absolute; top: 0; left: 0; width: 0; height: 50px; background-color: #fff; opacity: 0;}
.lnb .d-1 {display: inline-block;}
.lnb .d-1 > a {position: relative; display: block; width: 140px; height: 60px; font-family: 'nanum-square-b'; font-size: 18px; line-height: 60px;}
.lnb .d-1 > a span {position: relative; z-index: 2;}
.lnb .d-1.open > a {color: #c60824;}
.lnb .d-1.open .wrapSub {z-index: 3;}
.lnb .d-2 {float: left;}
.lnb .d-2 > li {float: left;}
.lnb .d-2 > li > a {position: relative; font-family: 'nanum-square-b'; font-size: 18px; line-height: 26px; color: #333;}
.lnb .d-2 > li > a:before {content:""; position: absolute; top: -15px; left: 0; display: block; width: 0px; height: 3px; background-color: #c60824; -webkit-transition: width 0.15s ease; -moz-transition: width 0.15s ease; -ms-transition: width 0.15s ease; -o-transition: width 0.15s ease; transition: width 0.15s ease;}
