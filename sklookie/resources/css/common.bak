@charset "utf-8";
@font-face {
    font-family: 'nanum-square-b';
    src: url('../font/nanumsquareb.eot');
    src: url('../font/nanumsquareb.eot?#iefix') format('embedded-opentype'),
         url('../font/nanumsquareb.woff2') format('woff2'),
         url('../font/nanumsquareb.woff') format('woff'),
         url('../font/nanumsquareb.ttf') format('truetype'),
         url('../font/nanumsquareb.svg#nanumsquarebregular') format('svg');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'nanum-square-r';
    src: url('../font/nanumsquarer.eot');
    src: url('../font/nanumsquarer.eot?#iefix') format('embedded-opentype'),
         url('../font/nanumsquarer.woff2') format('woff2'),
         url('../font/nanumsquarer.woff') format('woff'),
         url('../font/nanumsquarer.ttf') format('truetype'),
         url('../font/nanumsquarer.svg#nanumsquarerregular') format('svg');
    font-weight: normal;
    font-style: normal;
}

/*---------- reset ----------*/
html {margin: 0; padding: 0; overflow-y: scroll; overflow-x: hidden;}
body {margin: 0; padding: 0; position: relative; margin: 0; font-family: "nanum-square-r", sans-serif; font-size: 15px; line-height: 1.7; -webkit-text-size-adjust: none;}
html.open {overflow: hidden;}
html.bg-grey {background-color: #f4f4f4;}
header, h1, h2, h3, h4, h5, h6, section, p, ol, ul {margin: 0; padding: 0;}
button, select, input, textarea {font-family: "nanum-square-r", sans-serif;}
button {-moz-appearance: none; -webkit-appearance: none; appearance: none;}
li {margin: 0; padding: 0; list-style: none;}
a {text-decoration: none; color: inherit;}
a[href^="tel"] {color: inherit;}
img {border: 0;}
em, address {font-style: normal;}
hr {display: none;}
button {background: transparent; border: 0; -webkit-border-radius: 0; -moz-border-radius: 0; border-radius: 0; -webkit-appearance: none; appearance: none;}
caption {left: -9999px; width: 0.01em; height: 0.01em; font-size: 0.01em; line-height: 0.01em; visibility: hidden; overflow: hidden;}

/*---------- module ----------*/
/*accessibility */
.blind, .blind-p {position: absolute; left: -9999px; display: block; width: 1px; height: 1px; font-size: 0.1em; line-height: 0.01em; text-indent: -9999px; overflow: hidden;}
.w-hide, .w-hideBr {display: none !important;}

.skipnavi {position: relative; width: 1000px; height: 0; margin: 0 auto; z-index: 10000;}
.skipnavi a {position: absolute; top: -500px; left: 0; display: block; font-size: 1px; text-indent: -10000px; overflow: hidden;}
.skipnavi a:active, .skipnavi a:focus {position: absolute; top: 0; left: 0; display: block; width: 100%; height: 20px; padding: 5px 0; font-size: 14px; text-align: center; text-indent: 0; color: #fff; background-color: #e41c39; border: 1px solid #cecece;}
.skip-guide {position: absolute; top: 0; left: 0; font-size: 1px; text-indent: -10000px; overflow: hidden;}
.skip-guide:active, .skip-guide:focus {position: relative; top: auto; left: auto; width: 100%; height: 20px; padding: 5px 0; font-size: 14px; text-align: center; text-indent: 0; color: #fff; background-color: #e41c39; border: 1px solid #cecece; z-index: 100;}

/* em */
.em-weight {color: #f46a00;}
.em-weightier {color: #c60824;}

/* heading */
.h-t1 {margin-bottom: 20px; font-size: 28px; line-height: 36px; color: #222;}

/* icon */
.ico {display: inline-block; vertical-align: middle; text-indent: -99999px; background-image: url('../img/common/img_common.png'); background-repeat: no-repeat; -webkit-background-size: 800px 800px; background-size: 800px 800px;}
.ico._logoFoo {width: 130px; height: 52px; background-position: 0 0;}
.ico._fooBan_1 {width: 87px; height: 50px; background-position: -134px 0;}
.ico._fooBan_2 {width: 87px; height: 50px; background-position: -225px 0;}
.ico._fooBan_3 {width: 77px; height: 50px; background-position: -316px 0;}
.ico._global {width: 41px; height: 24px; background-position: -397px 0;}
.ico._global_2{width: 41px; height: 24px; background-position: -594px 0;}
.ico._go {width: 45px; height: 45px; background-position: -442px 0;}
.ico._plus_1 {width: 9px; height: 9px; margin: -2px 0 0 10px; background-position: -491px 0;}
.ico._arrow_1 {width: 4px; height: 7px; margin: -2px 0 0 10px; background-position: -504px 0;}
.ico._sitemap {width: 21px; height: 18px; background-position: -512px 0;}
.ico._sitemap_2{width: 21px; height: 18px; background-position: -639px 0;}
.ico._pause_1 {width: 12px; height: 15px; background-position: -537px 0;}
.ico._play_1 {width: 15px; height: 17px; background-position: -553px 0;}
.ico._close_1 {width: 18px; height: 18px; background-position: -572px 0;}
.ico._home {width: 16px; height: 18px; background-position: -664px 0;}

.ico-busi {display: inline-block; width: 50px; height: 50px; background: url(../img/main/ico_business_m.png) no-repeat 0 0; -webkit-background-size: 300px; background-size: 300px;}
.ico-busi._1 {background-position: 0 0;}
.ico-busi._2 {background-position: -50px 0;}
.ico-busi._3 {background-position: -100px 0;}
.ico-busi._4 {background-position: -150px 0;}
.ico-busi._5 {background-position: -200px 0;}
.ico-busi._6 {background-position: -250px 0;}

/* button */
.btn-play {display: inline-block; width: 15px; height: 17px; vertical-align: middle; text-indent: -99999px; background: url('../img/common/img_common.png') no-repeat -553px 0;}
.btn-stop {display: inline-block; width: 12px; height: 15px; vertical-align: middle; text-indent: -99999px; background: url('../img/common/img_common.png') no-repeat -537px 0;}
.btn-close {display: inline-block; width: 18px; height: 18px; vertical-align: middle; text-indent: -99999px; background: url('../img/common/img_common.png') no-repeat -572px 0;}
.btn-menu-m {display: none;}

/* txt */
.txt-t1 {font-size: 16px; line-height: 28px; text-align: justify; color: #999;}

/* img */
.img-bg {background-repeat: no-repeat; background-position: 0 0; -webkit-background-size: cover; background-size: cover;;}
.img-mn {display: block; width: 220px; height: 125px; overflow: hidden;}
.img-mn:after {content: ""; display: block; width: 220px; height: 125px; background: url(../img/main/img_mn.jpg) no-repeat 0 0;}
.img-mn._1-1:after {background-position: 0 0;}
.img-mn._1-2:after {background-position: -224px 0;}

/* diagram */
.dia-overview {width: 398px; height: 379px; margin: 40px auto 0; background: url(../img/intro/dia_overview_1.jpg) no-repeat 0 0;}
