.lnb .d-3 > li > a:after {content:""; display: block; width: 0; height: 1px; background-color: #c60824; -webkit-transition: width 0.2s ease; -moz-transition: width 0.2s ease; -ms-transition: width 0.2s ease; -o-transition: width 0.2s ease; transition: width 0.2s ease;}
.lnb .d-3 > li > a:hover, .lnb .d-3 > li > a:focus {color: #c60824;}
.lnb .d-3 > li > a:hover:after, .lnb .d-3 > li > a:focus:after {width: 100%;}
.lnb .wrapFocusMn {float: right; max-width: 560px; text-align: right; border-left: 1px solid #f2f2f2;}
.lnb .focusMn {display: inline-block; margin-left: 58px; text-align: left;}
